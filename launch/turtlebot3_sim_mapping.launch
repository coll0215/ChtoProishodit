<launch>
  <node name="map_server" pkg="map_server" type="map_server" args="$(find cringe_robot_package)/config/parkingmapfinal.yaml"/> 
  <include file="$(find cringe_robot_package)/launch/amcl.launch">
		<arg name="initial_pose_x" default="-3.0"/>
		<arg name="initial_pose_y" default="1.0"/>
		<arg name="initial_pose_a" default="0.0"/>
	</include>
  <include file="$(find cringe_robot_package)/launch/turtlebot3_for_fun.launch" />
  <!-- <include file="$(find cringe_robot_package)/launch/turtlebot3_my_gmapping.launch" /> -->
  <include file="$(find cringe_robot_package)/launch/turtlebot3_move_base.launch" />
  <param name="robot_description1" command="$(find xacro)/xacro --inorder $(find cringe_robot_package)/urdf/april_cube_demo.xacro" />
  <include file="$(find apriltag_ros)/launch/continuous_detection.launch"/>

  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find cringe_robot_package)/rviz/sim_mapping.rviz" /> 
  
</launch>


